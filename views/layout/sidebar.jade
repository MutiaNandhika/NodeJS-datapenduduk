aside.main-sidebar
    section.sidebar
        .user-panel
            .pull-left.image
                img(src='/adminlte/dist/img/mutia.jpg',alt='User Image').img-circle
            .pull-left.info
                p Mutia Nandhika
                a(href='#')
                    i.fa.fa-circle.text-success
                    | Online 
        form(action='#',method='get').sidebar-form
            .input-group
                input(type='text', name='q',placeholder='Search...').form-control
                span.input-group-btn
                    button(type='submit', name='search')#search-btn.btn.btn-flat
                        i.fa-fa-search
        ul.sidebar-menu 
            li.header
                | Main Navigation 
            li.treeview
                a(href='/wargas')
                    i.fa.fa-user
                    span
                        | Data Warga
                    span.pull-right-container

            li.treeview 
                a(href='/keluargas')
                    i.fa.fa-users 
                    span
                        | Data Kartu Keluarga
                    span.pull-right-container